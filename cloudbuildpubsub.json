{
  "steps": [
    {
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
      "args": [
        "gcloud",
        "functions",
        "deploy",
        "HelloWorld",
        "--gen2",
        "--region=us-central1",
        "--source=./cb_pubsub",
        "--trigger-topic=cbt-tpoic",
        "--runtime=go122",
        "--allow-unauthenticated",
        "--project=taas-automation"
      ]
    }
  ],
  "options": {
    "logging": "CLOUD_LOGGING_ONLY"
  }
}
