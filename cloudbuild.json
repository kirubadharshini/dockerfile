{
    "steps": [
     {
        "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
        "args": [
          "gcloud",
          "functions",
          "deploy",
          "HelloWorld",
           "--gen2",
           "--region=us-central1",
            "--source=./main.go",
           "--trigger-http",
           "--runtime=nodejs18",
           "--project=taas-automation"
           "options": {
                "logging": "CLOUD_LOGGING_ONLY"
           }
        ]
      }
     ]
   }
