{
    "steps": [
      {
        "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
        "args": [
          "gcloud",
          "functions",
          "deploy",
          "HelloWorld",
          "--gen2",
          "--region=us-central1",
          "--source=./cb_storage",
          "--trigger-bucket=taas-frontend-storage",
          "--runtime=go122",
          "--allow-unauthenticated",
          "--project=taas-automation"
        ]
      }
    ],
    "options": {
      "logging": "CLOUD_LOGGING_ONLY"
    }
  }
  